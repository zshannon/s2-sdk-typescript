{
  "name": "@zshannon/streamstore",
  "version": "0.22.5",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/s2-streamstore/s2-sdk-typescript"
  },
  "homepage": "https://s2.dev",
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "rm -rf dist && bun ../../scripts/generate-version.ts && tsc --project tsconfig.build.esm.json && tsc --project tsconfig.build.cjs.json && bun ../../scripts/postbuild.ts && attw --pack",
    "test": "vitest --run",
    "check": "tsc --noEmit",
    "gen:version": "bun ../../scripts/generate-version.ts",
    "example:browser": "bun examples/browser/serve.ts"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@biscuit-auth/biscuit-wasm": "^0.6.0-beta.1",
    "@noble/curves": "^2.0.1",
    "@noble/hashes": "^1.8.0",
    "@protobuf-ts/runtime": "^2.11.1",
    "@scure/base": "^2.0.0",
    "debug": "^4.4.3"
  }
}
